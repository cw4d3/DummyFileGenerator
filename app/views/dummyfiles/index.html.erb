<div class="ten columns offset-by-two">
  <div class="ten columns" id="details-box">
    <p>Final Details</p>
    <div class="final-details-wrapper">
      <%= label_tag(:size, "How large? (in MB, 999 max, numbers only please)", :id => 'final-details') %>
      <%= text_field_tag(:size) %>
      <!--<label id="final-details" for="size">How large? (in MB)</label>
      <input type="textarea" name="size" value="" size="3">-->
    </div>
      
    <div class="final-details-wrapper">
      <%= label_tag(:executable, "Disguise this file as an executable? (Creates an executable with false extension)", :id => 'final-details') %>
      <%= check_box_tag(:executable) %>      
      <!--<label id="final-details" for="executable">Disguise this file as an executable? (Creates an executable with false extension) </label>
      <input id="" type="checkbox" name="executable" value="">-->
    </div>
    
    <div class="final-details-wrapper">
      <%= submit_tag("Generate!", :class => "full-width remove-bottom") %>
      <!--<input class="full-width remove-bottom" type="submit" name="submit" value="Generate!">-->
    </div>
  </div>
</div>

